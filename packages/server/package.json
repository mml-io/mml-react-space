{
  "name": "server",
  "private": true,
  "version": "0.0.1",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "iterate": "concurrently \"node build.js --watch\" \"nodemon\"",
    "build": "rimraf ./build && node build.js --build",
    "start": "NODE_ENV=production node build/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"./src/**/*.{ts,}\" --max-warnings 0",
    "lint:fix": "eslint \"./src/**/*.{ts,}\" --fix"
  },
  "dependencies": {
    "@dolbyio/dolbyio-rest-apis-client": "4.1.0",
    "@mml-io/3d-web-experience-server": "0.20.0",
    "@mml-io/3d-web-user-networking": "0.20.0",
    "@mml-io/networked-dom-server": "^0.19.1",
    "canvas": "^2.11.2",
    "chokidar": "^3.6.0",
    "cors": "^2.8.5",
    "express": "4.19.2",
    "express-ws": "5.0.2",
    "http-proxy": "^1.18.1",
    "networked-dom-server": "0.14.0",
    "web-client": "0.0.1",
    "ws": "8.17.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.4",
    "@types/http-proxy": "^1.17.14",
    "@types/node": "^20.12.12",
    "nodemon": "^3.1.1"
  }
